{
	"name": "biosimreg-server",
	"version": "1.2.0",
	"description": "",
	"main": "server.js",
	"scripts": {
		"start": "node build/src/server.js",
		"db:pull": "prisma db pull && prisma generate",
		"db:push": "prisma db push && prisma generate",
		"dev": "ts-node-dev --respawn --transpile-only src/server.ts",
		"build": "npm run cleanup && tsc --pretty",
		"cleanup": "rm -fr build",
		"lint-staged": "lint-staged",
		"prepare": "husky install",
		"format": "npm run lint && npm run prettier",
		"lint": "npm run lint:check && npm run lint:fix",
		"lint:check": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts",
		"prettier": "npm run prettier:check && npm run prettier:fix",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"pretty-quick": "pretty-quick --staged"
	},
	"keywords": [],
	"author": "Enoch Klufio",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/enochmk/biosimreg-backend.git"
	},
	"bugs": {
		"url": "https://github.com/enochmk/biosimreg-backend/issues"
	},
	"homepage": "https://github.com/enochmk/biosimreg-backend#readme",
	"dependencies": {
		"@prisma/client": "^3.9.2",
		"@types/cookie-parser": "^1.4.2",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/md5": "^2.3.2",
		"@types/yup": "^0.29.13",
		"config": "^3.3.7",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.2",
		"helmet": "^5.0.2",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^8.5.1",
		"md5": "^2.3.0",
		"moment": "^2.29.1",
		"morgan": "^1.10.0",
		"mssql": "^8.0.2",
		"mysql": "^2.18.1",
		"nanoid": "^3.3.0",
		"winston": "^3.6.0",
		"xml2js": "^0.4.23",
		"xss": "^1.0.10",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.1",
		"@commitlint/config-conventional": "^16.2.1",
		"@types/config": "^0.0.41",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/hpp": "^0.2.2",
		"@types/moment": "^2.13.0",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.18",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.9.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.4",
